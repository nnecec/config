{
  "name": "@nnecec/config",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/nnecec/config.git"
  },
  "author": "nnecec(https://github.com/nnecec/)",
  "type": "module",
  "workspaces": [
    "packages/*",
    "fixtures/*",
    "website"
  ],
  "scripts": {
    "beachball": "beachball",
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "release": "npm run build && beachball publish"
  },
  "devDependencies": {
    "@nnecec/eslint-config": "workspace:*",
    "@nnecec/prettier-config": "workspace:*",
    "@nnecec/tsconfig": "workspace:*",
    "beachball": "^2.63.0",
    "eslint": "^10.0.2",
    "turbo": "^2.8.12",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.30.3",
  "beachball": {
    "access": "public"
  }
}
