{
  "name": "@nnecec/config",
  "version": "0.0.0",
  "private": true,
  "author": "nnecec(https://github.com/nnecec/)",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "beachball": "beachball",
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "release": "beachball publish"
  },
  "devDependencies": {
    "beachball": "^2.23.0",
    "eslint": "^8.18.0",
    "prettier": "^2.7.1",
    "turbo": "^1.2.16",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "packageManager": "pnpm@7.0.0"
}
