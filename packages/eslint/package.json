{
  "name": "@nnecec/eslint-config",
  "version": "0.6.4",
  "description": "The useful eslint config base on Standard. Support Vanilla JavaScript, TypeScript or React.",
  "license": "MIT",
  "author": "nnecec(https://github.com/nnecec/)",
  "scripts": {
    "build": "bunchee --sourcemap",
    "dev": "bunchee -w"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./legacy/*": "./legacy/*"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-i": "^2.29.0",
    "eslint-plugin-jsdoc": "^46.8.2",
    "eslint-plugin-json-files": "^4.0.0",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-perfectionist": "^2.2.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-plugin-unicorn": "^49.0.0",
    "globals": "^13.23.0",
    "yaml-eslint-parser": "^1.2.2"
  },
  "devDependencies": {
    "@nnecec/tsconfig": "workspace:*",
    "@types/eslint": "^8.44.6",
    "bunchee": "^3.9.1",
    "typescript": "^5.2.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
